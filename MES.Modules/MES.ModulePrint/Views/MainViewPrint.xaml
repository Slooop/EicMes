<UserControl x:Class="MES.ModulePrint.MainViewPrint"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MES.ModulePrint"
             mc:Ignorable="d" >
    <Grid >
        <TabControl Background="{DynamicResource Background_1}" >
            <!--管制卡打印-->
            <TabItem Header="管制卡打印" Margin="2,-2,-2,-1"  >
                <local:FlowCardPrint />
            </TabItem>

            <!--SN标签打印-->
            <TabItem Header="SN标签打印" Margin="2,-2,-2,-1">
                <local:SerialNumberPrint />
            </TabItem>

        </TabControl>

    </Grid>

</UserControl>
