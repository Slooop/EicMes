<UserControl x:Class="MES.ModuleBPM.LabPrint"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MES.ModuleBPM"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="800">
    <Grid Background="{DynamicResource AlternateBackgroundBrush}">
        <Grid.RowDefinitions>
            <RowDefinition Height="400"/>
            <RowDefinition />
        </Grid.RowDefinitions>
        <Grid Grid.Row="1" Background="{DynamicResource TabItemHotBorderBrush}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition Width="10"/>
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <DataGrid></DataGrid>
            <DataGrid Grid.Column="2"></DataGrid>
        </Grid>

        <Grid Background="{DynamicResource Background_1}"/>

    </Grid>
</UserControl>
